      </Fields>
      <Query>
        <DataSourceName>Advant</DataSourceName>
        <CommandText>SELECT     DeptMaster.Dept_ID, DeptMaster.DeptName, DeptMaster.DeptCode, DeptMaster.Del_Flag, DeptMaster.Institute_ID, DeptMaster.Branch_ID, 
                      InstituteMaster.InstituteName, BranchMaster.BranchName
FROM         DeptMaster LEFT OUTER JOIN
                      BranchMaster ON DeptMaster.Branch_ID = BranchMaster.Branch_ID LEFT OUTER JOIN
                      InstituteMaster ON DeptMaster.Institute_ID = InstituteMaster.Institute_ID
WHERE     (DeptMaster.Del_Flag = 0)
ORDER BY DeptMaster.DeptName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
