      <Query>
        <DataSourceName>Advant</DataSourceName>
        <CommandText>SELECT     rpt_LoanMaster.LoanIDCode, rpt_LoanMaster.Emp_Name, rpt_LoanMaster.LoanType, rpt_LoanMaster.LoanDate, rpt_LoanMaster.LoanAmt, 
                      rpt_LoanMaster.InterestRate, rpt_LoanMaster.ChequeDate, rpt_LoanMaster.MonthlyDed, rpt_LoanMaster.BalanceLoan, rpt_LoanMaster.StartDate, 
                      rpt_LoanMaster.Hold, rpt_LoanMaster.Del_Flag, rpt_LoanMaster.Expr1, rpt_LoanMaster.Institute_ID, rpt_LoanMaster.Branch_ID, 
                      BranchMaster.BranchName, InstituteMaster.InstituteName
FROM         rpt_LoanMaster LEFT OUTER JOIN
                      InstituteMaster ON rpt_LoanMaster.Institute_ID = InstituteMaster.Institute_ID LEFT OUTER JOIN
                      BranchMaster ON rpt_LoanMaster.Branch_ID = BranchMaster.Branch_ID
WHERE     (rpt_LoanMaster.Del_Flag = 0) AND (rpt_LoanMaster.Institute_ID = @Param1) AND (rpt_LoanMaster.Branch_ID = @Param2)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
